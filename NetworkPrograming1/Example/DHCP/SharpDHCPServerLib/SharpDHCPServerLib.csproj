<Project Sdk="Microsoft.NET.Sdk">
 <PropertyGroup>
   <TargetFrameworks>netstandard2.0;net40</TargetFrameworks>
   <PackageId>DotNetProjects.DhcpServer</PackageId>
   <Authors>DotNetProjects</Authors>
   <Company>DotNetProjects</Company>
   <Product>DotNetProjects.DhcpServer</Product>
   <PackageProjectUrl>https://github.com/dotnetprojects/sharp-dhcp-server-lib</PackageProjectUrl>
   <PackageLicenseExpression>MIT</PackageLicenseExpression>
   <FileVersion>1.0.0.0</FileVersion>
   <AssemblyVersion>1.0.0.0</AssemblyVersion>
   <Description>a fully managed DhcpServer</Description>
   <Copyright>2019</Copyright>
   <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
   <AssemblyName>DotNetProjects.DhcpServer</AssemblyName>
   <RootNamespace>DotNetProjects.DhcpServer</RootNamespace>
   <Version>1.0.0</Version>
 
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>

 
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>